maxHeap
minHeap

putInHeaps(int newNumber){


if(newNumber <= maxHeap.top)
   maxHeap.ADD(newNumber)
else if(newNumber => minHeap)
   minHeap.ADD(newNumber)

if((minHeap.size - maxHeap.size) > 2)
      minHeap.r
   


}

